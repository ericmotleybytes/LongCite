{
    "@note001": "Note: keys starting with @ generally ignored."
    "name": "LongCite",
    "namemsg": "longcite-extensionname",
    "version": "0.0.1",
    "author": [ "Eric Alan Christiansen" ],
    "url": "https://motleybytes.com/w/Extension:LongCite",
    "descriptionmsg": "longcite-desc",
    "license-name": "MIT",
    "type": "other",
    "AutoloadClasses": {
        "LongCiteMaster"    : "includes/LongCiteMaster.php",
        "LongCiteMessenger" : "includes/LongCiteMessenger.php"
    }
    "config_prefix": "wg",
	"ConfigRegistry": {
        "longcite": "GlobalVarConfig::newInstance"
	},
    "config": {
        "LongCiteSettingFoo": {
            "value": true,
            "path": false,
            "description": "Whether or not setting foo is enabled.",
            "descriptionmsg": "longcite-config-longcitesettingfoo",
            "public": true
        }
    },
	"MessagesDirs": {
		"LongCite": [
			"i18n"
		]
	},
    "ResourceModules": {
        "ext.longCite": {
            "styles": [
                "ext.longCite.css"
            ],
            "messages": [],
            "dependencies": []
        }
    },
    "ResourceFileModulePaths": {
        "localBasePath": "modules",
        "remoteExtPath": "LongCite/modules"
    },
    "manfest_version": 2
}
